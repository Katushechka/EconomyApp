<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.p1">

    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.USE_FINGERPRINT"></uses-permission>
    <uses-permission android:name="android.permission.USE_BIOMETRIC" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ShowIncomes"
            android:parentActivityName=".GeneralInfo"/>
        <activity android:name=".SetScan" />
        <activity android:name=".ShowExpense"
            android:parentActivityName=".GeneralInfo"/>
        <activity android:name=".ShowSelectedIncome"
            android:parentActivityName=".GeneralInfo"/>
        <activity android:name=".ShowSelectedExpense"
            android:parentActivityName=".GeneralInfo"/>
        <activity android:name=".Dateinterval" />
        <activity android:name=".ShowIncomesWithinDateinterval"
            android:parentActivityName=".GeneralInfo"/>
        <activity android:name=".ShowExpensesWithinDateinterval"
            android:parentActivityName=".GeneralInfo"/>
        <activity android:name=".Information"
            android:parentActivityName=".GeneralInfo"/>
        <activity android:name=".GeneralInfo"
            android:launchMode="singleTop"
            android:parentActivityName=".GeneralInfo"/>
        <activity android:name=".SetNote"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".SetIncome" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>